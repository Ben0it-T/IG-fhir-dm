# Parquet to DuckDB Configuration
# Copy this file to .env and customize the values

# Input directory containing Parquet files (relative to this directory)
PARQUET_DUCKDB_INPUT_DIRECTORY=../script/output

# Output directory for DuckDB database
PARQUET_DUCKDB_OUTPUT_DIRECTORY=./output

# Database configuration
PARQUET_DUCKDB_DATABASE_NAME=omop_cdm
PARQUET_DUCKDB_SCHEMA_NAME=omop

# Path to OMOP SQL files (relative to this directory)
PARQUET_DUCKDB_OMOP_SQL_DIRECTORY=../sql/omop

# Performance settings
PARQUET_DUCKDB_MEMORY_LIMIT=4GB
PARQUET_DUCKDB_THREADS=4
PARQUET_DUCKDB_BATCH_SIZE=10000

# Logging configuration
PARQUET_DUCKDB_LOG_LEVEL=INFO
PARQUET_DUCKDB_LOG_FILE=./logs/parquet_loader.log

# Processing options
PARQUET_DUCKDB_ENABLE_PROGRESS_BAR=true