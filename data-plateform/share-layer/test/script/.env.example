# HAPI FHIR Server Configuration
HAPI_FHIR_URL=http://localhost:8080/fhir
HAPI_FHIR_VERSION=8.4.0
FHIR_VERSION=R4
HAPI_DB_TYPE=PostgreSQL
HAPI_TIMEOUT=30
HAPI_MAX_RETRIES=3
HAPI_RETRY_DELAY=5

# Import Configuration
DATA_DIRECTORY=../mimic-iv-clinical-database-demo-on-fhir-2.1.0/fhir
FILE_PATTERN=*.ndjson.gz
BATCH_SIZE=1000
MAX_CONCURRENT_REQUESTS=5
POLL_INTERVAL=10
MAX_WAIT_TIME=3600

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=fhir_import.log